    "df = pd.read_csv('../data/cleaned_reviews.csv')\n",
    "\n",
    "# EDA\n",
    "plt.hist(df['Score'], bins=5)\n",
    "plt.xlabel('Score')\n",
    "plt.ylabel('Frequency')\n",
    "plt.title('Distribution of Scores')\n",
    "plt.show()\n"
   ]
